{
  "name": "natours",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "watch:sass": "sass starter/sass/main.scss starter/css/style.css -w",
    "compile:sass": "sass starter/sass/main.scss starter/css/style.comp.css",
    "concat:css": "concat -o starter/css/style.concat.css starter/css/icon-font.css starter/css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' starter/css/style.concat.css -o starter/css/style.prefix.css",
    "compress:css": "sass starter/css/style.prefix.css starter/css/style.css --output-style compressed",
    
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css"
    
  },
